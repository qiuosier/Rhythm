{
    "steps": [
        {
            "name": "gcr.io/cloud-builders/git",
            "args": ["submodule", "update", "--init", "--recursive"]
        },
        {
            "name": "gcr.io/cloud-builders/gsutil",
            "args": ["cp", "gs://qiu-nest.appspot.com/private.py", "rhythm/private.py"],
            "timeout": "600s"
        },
        {
            "name": "gcr.io/cloud-builders/gcloud",
            "args": ["app", "deploy"],
            "timeout": "600s"
        }
    ]
}